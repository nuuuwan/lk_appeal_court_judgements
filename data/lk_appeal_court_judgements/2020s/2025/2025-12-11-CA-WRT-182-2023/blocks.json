  {
    "page_number": 3,
    "bbox": [
      72.03,
      218.64,
      158.83,
      231.92
    ],
    "text": "by the Arbitrator.",
    "fonts": [
      "TimesNewRomanPSMT"
    ],
    "sizes": [
      12.0
    ]
  },
  {
